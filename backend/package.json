{
	"name": "cse-416-fall-25-binary-trees-backend",
	"private": true,
	"version": "1",
	"main": "index.ts",
	"scripts": {
		"build": "tsc && tsup",
		"dev": "node --import=tsx --watch src/index.ts",
		"start": "node dist/index.js"
	},
	"dependencies": {
		"cors": "2.8.5",
		"dayjs": "^1.11.18",
		"dotenv": "17.2.1",
		"express": "5.1.0",
		"helmet": "8.1.0",
		"http-status-codes": "2.3.0",
		"mongoose": "^8.18.1",
		"pino": "9.9.0",
		"pino-http": "10.5.0",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@types/cors": "2.8.19",
		"@types/express": "5.0.3",
		"pino-pretty": "13.1.1",
		"tsup": "8.5.0",
		"tsx": "4.20.4",
		"typescript": "5.9.2",
		"vite-tsconfig-paths": "5.1.4"
	},
	"tsup": {
		"entry": [
			"src/index.ts"
		],
		"outDir": "dist",
		"format": [
			"esm",
			"cjs"
		],
		"target": "es2020",
		"sourcemap": true,
		"clean": true,
		"dts": true,
		"splitting": false,
		"skipNodeModulesBundle": true
	},
	"packageManager": "pnpm@10.15.1",
	"engines": {
		"node": "22.x"
	}
}